{
	"documentRoot" : "public",
	"workerCount" : 0,
	"useLogColors" : true,
	"forceHttps": ${server.https.force},
	"http":
	{
		"port":8080
	},
	"https":
	{
		"port":8081,
		"certificate":"${server.https.certificateFile}",
		"key":"${server.https.keyFile}",
		"intermediate":"${server.https.intermediate}"
	},
	"localization":
	{
		"filePath": "public/js/data",
		"extension": "json"
	},
	"mysql":
	{
		"host":"${server.db.host}",
		"user":"${server.db.user}",
		"password":"${server.db.password}",
		"database":"${server.db.database}"
	},
	"share":
	{
		"title" : "${settings.og.title}",
		"description" : "${settings.og.description}",
		"image" : "${settings.og.image}",
		"url" : "${settings.og.url}"
	},
	"email":
	{
		"transport":"${server.email.transport}",
		"defaultSender":"${server.email.defaultSender}"
	}
}
